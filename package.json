{
  "name": "out-pressure",
  "version": "0.0.1",
  "description": "an example based on umijs + electron",
  "main": "./dist/main/main.js",
  "private": true,
  "build": {
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "mac": {
      "category": "your.app.category.out-pressure"
    },
    "linux": {},
    "directories": {
      "output": "release"
    },
    "appId": "out-pressure",
    "asar": false
  },
  "scripts": {
    "start:dev": "concurrently \"npm run start:renderer\" \"npm run start:main\"",
    "start": "npm run build-main-dev && electron ./dist/main/main.js",
    "start:main": "electron-webpack dev",
    "start:renderer": "cross-env APP_ROOT=src/renderer umi dev",
    "build-main-prod": "cross-env NODE_ENV=production webpack --config ./build/webpack.main.prod.config.js",
    "build-main-dev": "cross-env NODE_ENV=development webpack --config ./build/webpack.main.config.js",
    "build:renderer": "cross-env APP_ROOT=src/renderer umi build",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "debug:main": "electron --inspect=5858 -w ./dist/main/main.js",
    "pack": "npm run build:renderer && npm run build-main-prod && electron-builder --dir",
    "dist": "electron-builder",
    "prettier": "prettier --list-different \"./**/*.{ts,tsx,js,jsx,less}\"",
    "pi": "npm run build:renderer && npm run build-main-prod && electron-packager ./ --overwrite --platform=linux --arch=armv7l",
    "rebuild": "electron-rebuild -f -w serialport",
    "pi2": "electron-packager ./ --overwrite --platform=linux --arch=armv7l"
  },
  "keywords": [
    "Electron",
    "umi",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": {
    "name": "icezeros",
    "email": "icezeros@163.com"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "dva-logger": "^1.0.0",
    "electron": "^2.0.1",
    "electron-builder": "^20.39.0",
    "electron-debug": "^2.2.0",
    "electron-packager": "^14.0.1",
    "electron-rebuild": "^1.8.5",
    "electron-webpack": "^2.6.2",
    "prettier": "1.17.0",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "umi": "^2.6.17",
    "umi-plugin-react": "^1.7.6",
    "webpack": "4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "@types/nedb": "^1.8.8",
    "antd": "^3.19.6",
    "bizcharts": "^3.5.4",
    "d3-format": "^1.3.2",
    "d3-shape": "^1.3.5",
    "dayjs": "^1.8.15",
    "electron-is": "^3.0.0",
    "electron-log": "^3.0.6",
    "electron-store": "^3.3.0",
    "fs-extra": "^8.1.0",
    "getmac": "^1.4.6",
    "http-proxy": "^1.17.0",
    "iconv-lite": "^0.5.0",
    "inquirer": "^6.5.0",
    "lodash": "^4.17.14",
    "material-ui": "^0.20.2",
    "nedb": "^1.8.0",
    "nedb-async": "0.0.9",
    "nedb-promises": "^3.0.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "recharts": "^1.6.2",
    "serialport": "^7.1.5",
    "shelljs": "^0.8.3",
    "slash": "^3.0.0",
    "xls-write": "^1.0.7"
  },
  "electronWebpack": {
    "main": {
      "sourceDirectory": "src/main"
    },
    "renderer": {
      "sourceDirectory": null
    }
  }
}
