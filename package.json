{
  "name": "resin-electronjs",
  "version": "1.0.0",
  "description": "electronJS-based resin application",
  "main": "main.js",
  "scripts": {
    "dev": "NODE_ENV=development electron .",
    "lint": "eslint .",
    "start": "NODE_ENV=production startx ./node_modules/electron/dist/electron .",
    "local": "NODE_ENV=production electron .",
    "pack": "electron-packager ./ --overwrite --platform=linux --arch=armv7l"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/resin-io/resin-electronjs.git"
  },
  "keywords": [
    "resin",
    "io",
    "electron",
    "js"
  ],
  "dependencies": {
    "antd": "^3.12.3",
    "dayjs": "^1.8.10",
    "electron": "~9.4.0",
    "electron-packager": "^13.0.1",
    "electron-rebuild": "^1.8.2",
    "electron-serve": "^0.2.0",
    "iconv-lite": "^0.4.24",
    "lockfile": "^1.0.4",
    "nedb": "^1.8.0",
    "serialport": "^7.1.4"
  },
  "author": "Carlo Maria Curinga",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/resin-io/resin-electronjs/issues"
  },
  "homepage": "https://github.com/resin-io/resin-electronjs#readme",
  "jshintConfig": {
    "esversion": 6,
    "strict": true
  },
  "devDependencies": {
    "eslint": "^3.14.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0"
  }
}
